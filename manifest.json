{
    "manifest_version": 3,
    "name": "Productivity Manager",
    "version": "1.0",
    "description": "A productivity management extension built with MERN stack",
    "permissions": [
      "storage",
      "tabs",
      "activeTab"
    ],
    "action": {
      "default_popup": "index.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }